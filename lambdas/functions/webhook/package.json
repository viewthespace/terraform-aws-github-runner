{
  "name": "@terraform-aws-github-runner/function-webhook",
  "version": "1.0.0",
  "main": "lambda.ts",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev src/local.ts",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "lint": "yarn eslint src",
    "watch": "ts-node-dev --respawn --exit-child src/local.ts",
    "build": "ncc build src/lambda.ts -o dist",
    "dist": "yarn build && cd dist && zip ../webhook.zip index.js",
    "format": "prettier --write \"**/*.ts\"",
    "format-check": "prettier --check \"**/*.ts\"",
    "all": "yarn build && yarn format && yarn lint && yarn test"
  },
  "devDependencies": {
    "@octokit/webhooks-definitions": "^3.67.3",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@types/aws-lambda": "^8.10.125",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vercel/ncc": "0.38.1",
    "body-parser": "^1.20.2",
    "eslint": "^8.52.0",
    "eslint-plugin-prettier": "5.0.1",
    "express": "^4.18.2",
    "jest": "^29.5",
    "jest-mock": "^29.5.0",
    "nock": "^13.3.6",
    "prettier": "3.0.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.436.0",
    "@middy/core": "^3.6.2",
    "@octokit/rest": "^20.0.1",
    "@octokit/types": "^12.1.1",
    "@octokit/webhooks": "^13.0.1",
    "@terraform-aws-github-runner/aws-powertools-util": "*",
    "@terraform-aws-github-runner/aws-ssm-util": "*",
    "aws-lambda": "^1.0.7"
  }
}
